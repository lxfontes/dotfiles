do_docker_catalog() {
  curl -s 'https://docker.internal.digitalocean.com/v2/_catalog' | python -mjson.tool
}

do_cthulhu() {
  cd $HOME/code/src/github.internal.digitalocean.com/digitalocean/cthulhu
}

do_l0g() {
  docker run -it docker.internal.digitalocean.com/observability/l0g:latest $*
}

export KITCHEN_LOCAL_YAML=$HOME/.kitchen/config.yml
# silence doge
export SYSLOG_OFF=true
